    border-color: transparent currentColor transparent transparent;
    border-width: 1rem 1.375rem 1.375rem 0;
    left: -1.375rem;
    top: calc(50% - 18px);
  }
  .border-chevron-y-left::after {
    border-color: transparent currentColor transparent transparent;
    border-width: 0.875rem 1.1875rem 1.1875rem 0;
    left: -1.0625rem;
    top: calc(50% - 16px);
  }
}

